{
  "name": "@abcnews/components-builder",
  "version": "0.0.9",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "storybook": "storybook dev -p 6006 --ci",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-svelte-csf": "^5.0.0-next.0",
    "@storybook/blocks": "^8.6.12",
    "@storybook/svelte": "^8.6.12",
    "@storybook/sveltekit": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^20",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "storybook": "^8.6.12",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.6"
  },
  "keywords": [
    "svelte"
  ],
  "dependencies": {
    "@abcnews/alternating-case-to-object": "^3.2.0",
    "@abcnews/svelte-scrollyteller": "github:abcnews/svelte-scrollyteller#feature/future-news",
    "async": "^3.2.6",
    "bootstrap-icons": "^1.11.3",
    "jszip": "^3.10.1",
    "svelte-bootstrap-icons": "^3.1.2"
  }
}
